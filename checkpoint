ALGORYTHME SOMME TAB ARRAY_OF INTEGER

VAR 
    tab1 : INTEGER
    tab2 : INTEGER
    i := 0 : INTEGER
    j :=0 : INTEGER
    sum := 0 : INTEGER
BEGIN
 FOR  i FROM 0 TO tab1.length DO
 FOR  j FROM 0 TO tab2.length DO

 IF (tab1[i] === tab2[j]) DO
      
      sum := sum + tab1[i] + tab2[j]
 END IF
 END FOR 
 END FOR  

END